{
  "name": "web-app",
  "scripts": {
    "prepare": "husky || true",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "rm -rf .next/cache",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --runInBand",
    "infra:deploy": "serverless deploy --stage ${STAGE:-prod} --region ${AWS_REGION:-us-west-1}",
    "infra:remove": "serverless remove --stage ${STAGE:-prod} --region ${AWS_REGION:-us-west-1}",
    "test:selenium:intake": "node tests/selenium/intake.e2e.js",
    "test:selenium:client-create": "node tests/selenium/client-create.e2e.js",
    "test:selenium:agency-wizard": "node tests/selenium/agency-wizard.e2e.js",
    "test:selenium:recruiter-wizard": "node tests/selenium/recruiter-wizard.e2e.js",
    "test:selenium:ai-prompts": "node tests/selenium/ai-prompts.e2e.js",
    "test:selenium:lists": "node tests/selenium/lists.e2e.js",
    "test:selenium:athlete-profile": "node tests/selenium/athlete-profile.e2e.js",
    "test:selenium:login-ghl": "node tests/selenium/login-ghl.e2e.js",
    "test:selenium:login-ghl-negative": "node tests/selenium/login-ghl-negative.e2e.js",
    "test:selenium:intake-negative": "node tests/selenium/intake-negative.e2e.js",
    "test:selenium:athlete-profile-public": "node tests/selenium/athlete-profile-public.e2e.js",
    "test:selenium:all": "npm-run-all --sequential test:selenium:intake test:selenium:intake-negative test:selenium:client-create test:selenium:agency-wizard test:selenium:recruiter-wizard test:selenium:ai-prompts test:selenium:lists test:selenium:athlete-profile test:selenium:athlete-profile-public test:selenium:login-ghl test:selenium:login-ghl-negative",
    "infra:seed": "node scripts/seed-dynamo.mjs"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.687.0",
    "@aws-sdk/client-s3": "^3.966.0",
    "@aws-sdk/lib-dynamodb": "^3.687.0",
    "@aws-sdk/s3-request-presigner": "^3.966.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/system": "^7.3.5",
    "@mui/x-data-grid": "^8.19.0",
    "@sentry/aws-serverless": "^10.32.1",
    "@sentry/nextjs": "^10.32.1",
    "@tanstack/react-query": "^5.90.10",
    "bcryptjs": "^2.4.3",
    "driver.js": "^1.4.0",
    "googleapis": "^166.0.0",
    "next": "^16.0.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-quill-new": "^3.7.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/aws-lambda": "^8.10.145",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "29.5.12",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/selenium-webdriver": "^4",
    "cheerio": "^1.1.2",
    "chromedriver": "142",
    "cross-env": "^10.1.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "npm-run-all": "^4.1.5",
    "selenium-webdriver": "^4.38.0",
    "husky": "^9.1.7",
    "serverless": "^4.4.11",
    "serverless-esbuild": "^1.52.1",
    "ts-jest": "29.2.5",
    "typescript": "^5.9.3"
  }
}
